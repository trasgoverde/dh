        <addForeignKeyConstraint baseColumnNames="calbum_id"
                                 baseTableName="photo"
                                 constraintName="fk_photo__calbum_id"
                                 referencedColumnNames="id"
                                 referencedTableName="calbum"/>
    </changeSet>
</databaseChangeLog>
